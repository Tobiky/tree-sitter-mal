{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mal",
  "word": "identity",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "declaration"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": "[^\\r\\n\\u2028\\u2029]*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "include_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "category_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "define_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "associations_declaration"
        }
      ]
    },
    "include_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "include"
        },
        {
          "type": "FIELD",
          "name": "file",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "category_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "category"
        },
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "FIELD",
          "name": "meta",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "meta"
            }
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "define_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "associations_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "associations"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "association"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "association": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "left_id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "left_field_id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "FIELD",
          "name": "left_mult",
          "content": {
            "type": "SYMBOL",
            "name": "multiplicity"
          }
        },
        {
          "type": "STRING",
          "value": "<--"
        },
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": "-->"
        },
        {
          "type": "FIELD",
          "name": "right_mult",
          "content": {
            "type": "SYMBOL",
            "name": "multiplicity"
          }
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "right_field_id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "FIELD",
          "name": "right_id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "FIELD",
          "name": "meta",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "meta"
            }
          }
        }
      ]
    },
    "multiplicity": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_multiplicity_atom"
        },
        {
          "type": "SYMBOL",
          "name": "multiplicity_range"
        }
      ]
    },
    "_multiplicity_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "star"
        }
      ]
    },
    "multiplicity_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "start",
          "content": {
            "type": "SYMBOL",
            "name": "_multiplicity_atom"
          }
        },
        {
          "type": "STRING",
          "value": ".."
        },
        {
          "type": "FIELD",
          "name": "end",
          "content": {
            "type": "SYMBOL",
            "name": "_multiplicity_atom"
          }
        }
      ]
    },
    "meta": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": "info"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "info",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "(?:\\\\\"|[^\"])*"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "integer": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+"
      }
    },
    "identity": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9_]+"
      }
    },
    "star": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "*"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\r\\n]+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [
    [
      "association"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
