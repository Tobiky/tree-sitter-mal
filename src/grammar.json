{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mal",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "declaration"
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": "[^\\r\\n\\u2028\\u2029]*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/*"
            },
            {
              "type": "PATTERN",
              "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
            },
            {
              "type": "STRING",
              "value": "/"
            }
          ]
        }
      ]
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "include_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "category_declaration"
        }
      ]
    },
    "include_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "include"
        },
        {
          "type": "FIELD",
          "name": "file",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "category_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "category"
        },
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identity"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "(?:\\\\\"|[^\"])*"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "identity": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9_]+"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\r\\n]+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
